{
  "name": "@newordergame/ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^2.17.1",
    "@newordergame/common": "file:../common",
    "aws-amplify": "^4.3.22",
    "formik": "^2.2.9",
    "leaflet": "^1.8.0",
    "loglevel": "^1.8.0",
    "pixi.js": "^6.5.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-leaflet": "^4.0.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.5.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.0",
    "@types/leaflet": "^1.7.9",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "concurrently": "^7.2.0",
    "cypress": "^9.6.1",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build && cd aws && npm run deploy; cd ..",
    "update-common": "npm i -S @newordergame/common",
    "prettier": "npx prettier --write .",
    "cypress:open": "npx cypress open",
    "cypress:install": "npx cypress install",
    "cy:install": "cypress install",
    "cy:open:first": "cypress open --config-file cypress/cy-user1.json",
    "cy:open:second": "cypress open --config-file cypress/cy-user2.json",
    "cy:run:user1": "cypress run --config-file cypress/cy-user1.json",
    "cy:run:user2": "cypress run --config-file cypress/cy-user2.json",
    "cy:run:all": "concurrently npm:cy:run:user1 npm:cy:run:user2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
