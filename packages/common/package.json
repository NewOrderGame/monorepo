{
  "name": "@newordergame/common",
  "version": "1.0.57",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "yarn watch",
    "build": "npx tsc",
    "watch": "npx tsc --watch | sed -u 's/\\x1Bc//g'",
    "ship": "yarn publish-package",
    "publish-package": "yarn build && yarn publish --patch && yarn upgrade-package",
    "upgrade-package": "cd ../../ && yarn upgrade-common && cd -",
    "lint": "eslint ./src --config ./.eslintrc.js --no-cache",
    "format": "prettier --write src/**/*.ts",
    "test": "yarn build && jest",
    "type-check": "tsc --noEmit",
    "verify": "yarn format && yarn type-check && yarn lint && yarn test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "yup": "^0.32.11"
  }
}
